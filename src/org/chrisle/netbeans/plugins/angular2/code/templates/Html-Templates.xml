<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE codetemplates PUBLIC "-//NetBeans//DTD Editor Code Templates settings 1.0//EN" "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">
<codetemplates>
    <codetemplate abbreviation="aClass" xml:space="preserve">
        <code><![CDATA[[class]="${expression}"]]></code>
        <description><![CDATA[Angular [class] binding]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgClass" xml:space="preserve">
        <code><![CDATA[[ngClass]="{${cssClass}: ${expression}}"]]></code>
        <description><![CDATA[Angular ngClass]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgContainer" xml:space="preserve">
        <code><![CDATA[<ng-container ${cursor}></ng-container]]></code>
        <description><![CDATA[Angular ng-container]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgContent" xml:space="preserve">
        <code><![CDATA[<ng-content select="${selector}"></ng-content]]></code>
        <description><![CDATA[Angular ng-content]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgFor" xml:space="preserve">
        <code><![CDATA[*ngFor="let ${item} of ${list}"]]></code>
        <description><![CDATA[Angular *ngFor]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgForAsync" xml:space="preserve">
        <code><![CDATA[*ngFor="let ${item} of ${stream} | async as ${list}"]]></code>
        <description><![CDATA[Angular *ngForAsync]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgForTrackBy" xml:space="preserve">
        <code><![CDATA[*ngFor="let ${item} of ${list}; trackBy:${item}.id"]]></code>
        <description><![CDATA[Angular *ngFor with trackBy]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgIf" xml:space="preserve">
        <code><![CDATA[*ngIf="${expression}"]]></code>
        <description><![CDATA[Angular *ngIf]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgIfElse" xml:space="preserve">
        <code><![CDATA[*ngIf="${expression};else ${templateName}"]]></code>
        <description><![CDATA[Angular *ngIfElse]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgModel" xml:space="preserve">
        <code><![CDATA[[(ngModel)]="${binding}"]]></code>
        <description><![CDATA[Angular ngModel]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgStyle" xml:space="preserve">
        <code><![CDATA[[ngStyle]="{${style}: ${expression}}"]]></code>
        <description><![CDATA[Angular ngStyle]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgSwitch" xml:space="preserve">
        <code><![CDATA[<div [ngSwitch]="${conditionExpression}">
    <div *ngSwitchCase="${expression}">${output}</div>
    <div *ngSwitchDefault>${output2}</div>
</div>]]></code>
        <description><![CDATA[Angular ngSwitch snippet]]></description>
    </codetemplate>
    <codetemplate abbreviation="aNgTemplate" xml:space="preserve">
        <code><![CDATA[<ng-template [ngTemplateOutlet]="${outlet}" [ngOutletContext]="${context}"></ng-template]]></code>
        <description><![CDATA[Angular ng-template]]></description>
    </codetemplate>
    <codetemplate abbreviation="aPrej" xml:space="preserve">
        <code><![CDATA[<pre>{{${model} | json}}</pre>]]></code>
        <description><![CDATA[Angular pre debug | json]]></description>
    </codetemplate>
    <codetemplate abbreviation="aPreja" xml:space="preserve">
        <code><![CDATA[<pre>{{${model} | async | json}}</pre>]]></code>
        <description><![CDATA[Angular pre debug | async | json]]></description>
    </codetemplate>
    <codetemplate abbreviation="aRouterLink" xml:space="preserve">
        <code><![CDATA[[routerLink]="['${routePath}']"]]></code>
        <description><![CDATA[Angular routerLink]]></description>
    </codetemplate>
    <codetemplate abbreviation="aRouterLinkParam" xml:space="preserve">
        <code><![CDATA[[routerLink]="['${routePath}', ${routeParameterValue}]"]]></code>
        <description><![CDATA[Angular routerLink]]></description>
    </codetemplate>
    <codetemplate abbreviation="aSelect" xml:space="preserve">
        <code><![CDATA[<select [(ngModel)]="${model}">
<option *ngFor="let ${item} of ${list} [value]="${item}">{{${item}}}</option>
</select>]]></code>
        <description><![CDATA[<select> control with ngModel]]></description>
    </codetemplate>
    <codetemplate abbreviation="aStyle" xml:space="preserve">
        <code><![CDATA[[style.${property}]="${expression}"]]></code>
        <description><![CDATA[Angular [style] binding]]></description>
    </codetemplate>
</codetemplates>
